extends sidebar
block div#mycontent
 form(action="/webhook/updatewebhook",method="post")
  input(type="hidden",name="companyname",value = mydata.companyname)
  div.caption
    i(class = "glyphicon glyphicon-link")
      span 
        |Conference Webhook
    div.bodycontainer.table-scrollable
      table(class = "table table-striped table-bordered table-hover",id = "agents")
        tbody
            - for(i in mydata) {
                tr
                  td=i
                  td=mydata[i]
              - }
      
  div.caption
    i(class = "glyphicon glyphicon-pencil")
      span 
        |Update Webhook
    div
      input(type="text",value =mydata.webhook,name="newwebhook" size="50" style="margin-left:5px;")
      button(class = "btn btn-info" type="submit" style="margin-left:5px;")
                              | Submit
    